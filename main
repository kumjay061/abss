  <script>
    function insertSpaces(element) {
      var value = element.value.replace(/\s/g, ''); // Remove existing spaces
      var formattedValue = '';

      for (var i = 0; i < value.length; i++) {
        formattedValue += value[i];

        if ((i + 1) % 1 === 0 && (i + 1) !== value.length) {
          formattedValue += ' '; // Add space after each character
        }
      }

      element.value = formattedValue;
    }
  </script>             </div>


